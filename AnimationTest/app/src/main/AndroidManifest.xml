<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.animationtest">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".valueanimation.ValueAnimationActivity" />
        <activity android:name=".viewanimation.ViewAnimationActivity" />
        <activity android:name=".viewanimation.ScaleAnimationActivity" />
        <activity android:name=".viewanimation.AlphaAnimationActivity" />
        <activity android:name=".viewanimation.RotateAnimationActivity" />
        <activity android:name=".viewanimation.TranslateAnimationActivity" />
        <activity android:name=".viewanimation.SetAnimationActivity" />
        <activity android:name=".viewanimation.TestRelativeToParentActivity" />
        <activity android:name=".viewanimation.FrameAnimationActivity" />
        <activity android:name=".valueanimation.ValueAnimationOfObjectActivity" />
        <activity android:name=".valueanimation.ValueAnimationMenuActivity" />
        <activity android:name=".valueanimation.ObjectAnimationActivity" />
    </application>

</manifest>